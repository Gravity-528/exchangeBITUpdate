{
    "routes": [
      {
        "handle": "filesystem"
      },
      {
        "src": "/api/.*",
        "dest": "https://exchbit.onrender.com/:splat",
        "methods": ["GET", "POST", "PUT", "DELETE"]
      },
      {
        "src": "/assets/(.*)",
        "headers": {
          "Content-Type": "text/css"
        },
        "dest": "/assets/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  